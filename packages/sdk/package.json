{
  "name": "@chatbotkit/sdk",
  "version": "0.2.0",
  "description": "The fastest way to build advanced AI chat bots",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatbotkit/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chatbotkit/node-sdk/issues"
  },
  "homepage": "https://github.com/chatbotkit/node-sdk#readme",
  "author": "",
  "keywords": [
    "gpt3",
    "gpt4",
    "chat",
    "chatbot",
    "sdk",
    "chatbotkit",
    "serverless",
    "edge"
  ],
  "type": "module",
  "main": "./dist/esm/index.js",
  "exports": {
    "./client.js": {
      "require": {
        "types": "./dist/cjs/client.d.ts",
        "default": "./dist/cjs/client.cjs"
      },
      "import": {
        "types": "./dist/esm/client.d.ts",
        "default": "./dist/esm/client.js"
      }
    },
    "./conversation/index.js": {
      "require": {
        "types": "./dist/cjs/conversation/index.d.ts",
        "default": "./dist/cjs/conversation/index.cjs"
      },
      "import": {
        "types": "./dist/esm/conversation/index.d.ts",
        "default": "./dist/esm/conversation/index.js"
      }
    },
    "./conversation/v1.js": {
      "require": {
        "types": "./dist/cjs/conversation/v1.d.ts",
        "default": "./dist/cjs/conversation/v1.cjs"
      },
      "import": {
        "types": "./dist/esm/conversation/v1.d.ts",
        "default": "./dist/esm/conversation/v1.js"
      }
    },
    "./conversation/v2.js": {
      "require": {
        "types": "./dist/cjs/conversation/v2.d.ts",
        "default": "./dist/cjs/conversation/v2.cjs"
      },
      "import": {
        "types": "./dist/esm/conversation/v2.d.ts",
        "default": "./dist/esm/conversation/v2.js"
      }
    },
    "./fetch.js": {
      "require": {
        "types": "./dist/cjs/fetch.d.ts",
        "default": "./dist/cjs/fetch.cjs"
      },
      "import": {
        "types": "./dist/esm/fetch.d.ts",
        "default": "./dist/esm/fetch.js"
      }
    },
    "./index.js": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs_rename": "find ./dist/cjs -name '*.js' -exec rename .js .cjs {} +",
    "build:cjs_rewrite_cjs": "find ./dist/cjs -name '*.cjs' -exec sed -i 's/require(\"\\(.*\\)\\.js\")/require(\"\\1.cjs\")/g' {} +",
    "build:cjs_rewrite_ts": "find ./dist/cjs -name '*.ts' -exec sed -i 's/from \"\\(.*\\)\\.js\"/from \"\\1.cjs\"/g' {} +",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:exports": "node scripts/build-exports.js",
    "test": "true"
  },
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/isomorphic-fetch": "^0.0.36",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  }
}
