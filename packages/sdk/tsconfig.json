{
  "extends": "@system/toolchain/tsconfig.build.json",
  "compilerOptions": {
    "rootDir": "./src"
  },
  "include": ["src/**/*.ts", "src/**/*.js"],
  "exclude": ["dist", "dist.esm", "dist.cjs"],
  "typedocOptions": {
    "entryPoints": [
      "src/index.js",
      "src/usage/index.js",
      "src/usage/v1.js",
      "src/file/index.js",
      "src/file/v1.js",
      "src/bot/index.js",
      "src/bot/v1.js",
      "src/bot/session/index.js",
      "src/bot/session/v1.js",
      "src/skillset/index.js",
      "src/skillset/v1.js",
      "src/skillset/ability/index.js",
      "src/skillset/ability/v1.js",
      "src/dataset/index.js",
      "src/dataset/v1.js",
      "src/dataset/record/index.js",
      "src/dataset/record/v1.js",
      "src/dataset/file/index.js",
      "src/dataset/file/v1.js",
      "src/conversation/index.js",
      "src/conversation/v1.js",
      "src/conversation/message/index.js",
      "src/conversation/message/v1.js",
      "src/conversation/session/index.js",
      "src/conversation/session/v1.js",
      "src/integration/index.js",
      "src/integration/sitemap/index.js",
      "src/integration/sitemap/v1.js",
      "src/integration/whatsapp/index.js",
      "src/integration/whatsapp/v1.js",
      "src/magic/index.js",
      "src/magic/v1.js",
      "src/partner/index.js",
      "src/partner/v1.js",
      "src/partner/user/index.js",
      "src/partner/user/v1.js",
      "src/partner/user/token/index.js",
      "src/partner/user/token/v1.js"
    ],
    "excludePrivate": true,
    "excludeInternal": true,
    "excludeExternals": true
  }
}
