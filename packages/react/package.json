{
  "name": "@chatbotkit/react",
  "version": "1.5.3",
  "description": "The fastest way to build advanced AI chat bots",
  "license": "ISC",
  "engines": {
    "node": ">=18.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatbotkit/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chatbotkit/node-sdk/issues"
  },
  "homepage": "https://github.com/chatbotkit/node-sdk#readme",
  "author": "",
  "keywords": [
    "gpt3",
    "gpt4",
    "chat",
    "chatbot",
    "chatbotkit",
    "react"
  ],
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    "./components/AutoScroller": {
      "types": "./types/components/AutoScroller.d.ts",
      "import": "./src/components/AutoScroller.js"
    },
    "./components/AutoScroller.js": {
      "types": "./types/components/AutoScroller.d.ts",
      "import": "./src/components/AutoScroller.js"
    },
    "./components/AutoTextarea": {
      "types": "./types/components/AutoTextarea.d.ts",
      "import": "./src/components/AutoTextarea.js"
    },
    "./components/AutoTextarea.js": {
      "types": "./types/components/AutoTextarea.d.ts",
      "import": "./src/components/AutoTextarea.js"
    },
    "./components/ChatInput": {
      "types": "./types/components/ChatInput.d.ts",
      "import": "./src/components/ChatInput.js"
    },
    "./components/ChatInput.js": {
      "types": "./types/components/ChatInput.d.ts",
      "import": "./src/components/ChatInput.js"
    },
    "./components/ChatMessage": {
      "types": "./types/components/ChatMessage.d.ts",
      "import": "./src/components/ChatMessage.js"
    },
    "./components/ChatMessage.js": {
      "types": "./types/components/ChatMessage.d.ts",
      "import": "./src/components/ChatMessage.js"
    },
    "./components/ChatMessages": {
      "types": "./types/components/ChatMessages.d.ts",
      "import": "./src/components/ChatMessages.js"
    },
    "./components/ChatMessages.js": {
      "types": "./types/components/ChatMessages.d.ts",
      "import": "./src/components/ChatMessages.js"
    },
    "./hooks/useConversationManager": {
      "types": "./types/hooks/useConversationManager.d.ts",
      "import": "./src/hooks/useConversationManager.js"
    },
    "./hooks/useConversationManager.js": {
      "types": "./types/hooks/useConversationManager.d.ts",
      "import": "./src/hooks/useConversationManager.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./index": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./index.js": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./utils/object": {
      "types": "./types/utils/object.d.ts",
      "import": "./src/utils/object.js"
    },
    "./utils/object.js": {
      "types": "./types/utils/object.d.ts",
      "import": "./src/utils/object.js"
    },
    "./utils/string": {
      "types": "./types/utils/string.d.ts",
      "import": "./src/utils/string.js"
    },
    "./utils/string.js": {
      "types": "./types/utils/string.d.ts",
      "import": "./src/utils/string.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs_rename": "node ../../scripts/rename-cjs.js",
    "build:cjs_rewrite_cjs": "node ../../scripts/rewrite-cjs.js",
    "build:cjs_rewrite_ts": "node ../../scripts/rewrite-ts.js",
    "build:docs": "typedoc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:exports": "node ../../scripts/create-standard-exports.js",
    "build:types": "tsc -p tsconfig.types.json",
    "check": "tsc -p tsconfig.check.json",
    "clean": "run-p clean:*",
    "clean:buildinfo": "rimraf *.tsbuildinfo",
    "clean:dists": "rimraf dist",
    "clean:exports": "run-s build:exports",
    "clean:node_modules": "rimraf node_modules",
    "clean:types": "rimraf types",
    "lint": "eslint .",
    "postpack": "node ../../scripts/create-standard-exports.js",
    "prepack": "node ../../scripts/create-dist-exports.js",
    "test": "true"
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@chatbotkit/sdk": "workspace:*",
    "tslib": "^2.6.2",
    "react-markdown": "^9.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0"
  }
}
