{
  "name": "@chatbotkit/react",
  "version": "0.8.0",
  "description": "The fastest way to build advanced AI chat bots",
  "license": "ISC",
  "engines": {
    "node": ">=18.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatbotkit/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chatbotkit/node-sdk/issues"
  },
  "homepage": "https://github.com/chatbotkit/node-sdk#readme",
  "author": "",
  "keywords": [
    "gpt3",
    "gpt4",
    "chat",
    "chatbot",
    "chatbotkit",
    "react"
  ],
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    "./components/AutoScroller": {
      "import": {
        "default": "./src/components/AutoScroller.js"
      }
    },
    "./components/AutoScroller.js": {
      "import": {
        "default": "./src/components/AutoScroller.js"
      }
    },
    "./components/AutoTextarea": {
      "import": {
        "default": "./src/components/AutoTextarea.js"
      }
    },
    "./components/AutoTextarea.js": {
      "import": {
        "default": "./src/components/AutoTextarea.js"
      }
    },
    "./hooks/useConversationManager": {
      "import": {
        "default": "./src/hooks/useConversationManager.js"
      }
    },
    "./hooks/useConversationManager.js": {
      "import": {
        "default": "./src/hooks/useConversationManager.js"
      }
    },
    "./hooks/useConversationManager2": {
      "import": {
        "default": "./src/hooks/useConversationManager2.js"
      }
    },
    "./hooks/useConversationManager2.js": {
      "import": {
        "default": "./src/hooks/useConversationManager2.js"
      }
    },
    ".": {
      "import": {
        "default": "./src/index.js"
      }
    },
    "./index": {
      "import": {
        "default": "./src/index.js"
      }
    },
    "./index.js": {
      "import": {
        "default": "./src/index.js"
      }
    },
    "./utils/class": {
      "import": {
        "default": "./src/utils/class.js"
      }
    },
    "./utils/class.js": {
      "import": {
        "default": "./src/utils/class.js"
      }
    },
    "./utils/string": {
      "import": {
        "default": "./src/utils/string.js"
      }
    },
    "./utils/string.js": {
      "import": {
        "default": "./src/utils/string.js"
      }
    }
  },
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs_rename": "node ../../scripts/rename-cjs.js",
    "build:cjs_rewrite_cjs": "node ../../scripts/rewrite-cjs.js",
    "build:cjs_rewrite_ts": "node ../../scripts/rewrite-ts.js",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:exports": "node ../../scripts/build-exports.js",
    "check": "tsc --noEmit",
    "destroy": "run-s destroy:*",
    "destroy:exports": "node ../../scripts/destroy-exports.js",
    "postpublish": "run-s destroy:*",
    "test": "true"
  },
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "@chatbotkit/sdk": "*"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/react": "^18.2.6",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  }
}
