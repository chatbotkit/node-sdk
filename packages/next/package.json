{
  "name": "@chatbotkit/next",
  "version": "0.0.0",
  "description": "Utility library for ChatbotKit for Next.js",
  "license": "ISC",
  "engines": {
    "node": ">=18.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatbotkit/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chatbotkit/node-sdk/issues"
  },
  "homepage": "https://github.com/chatbotkit/node-sdk#readme",
  "author": "",
  "keywords": [
    "chatbotkit",
    "cbk",
    "next.js"
  ],
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    "./edge": {
      "import": {
        "default": "./src/edge.js"
      }
    },
    "./edge.js": {
      "import": {
        "default": "./src/edge.js"
      }
    },
    ".": {
      "import": {
        "default": "./src/index.js"
      }
    },
    "./index": {
      "import": {
        "default": "./src/index.js"
      }
    },
    "./index.js": {
      "import": {
        "default": "./src/index.js"
      }
    }
  },
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs_rename": "node ../../scripts/rename-cjs.js",
    "build:cjs_rewrite_cjs": "node ../../scripts/rewrite-cjs.js",
    "build:cjs_rewrite_ts": "node ../../scripts/rewrite-ts.js",
    "build:esm": "tsc -p tsconfig.esm.json",
    "check": "tsc --noEmit",
    "clean": "run-s clean:*",
    "clean:dists": "rimraf dist",
    "export": "node ../../scripts/create-exports.js",
    "lint": "eslint .",
    "postpack": "node ../../scripts/create-exports.js",
    "prepack": "node ../../scripts/build-exports.js",
    "test": "true"
  },
  "dependencies": {},
  "devDependencies": {
    "@system/toolchain": "*"
  }
}
