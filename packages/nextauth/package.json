{
  "name": "@chatbotkit/nextauth",
  "version": "1.28.0",
  "description": "ChatBotKit adapter for NextAuth.js to make conversational AI bots with authentication and authorization",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatbotkit/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chatbotkit/node-sdk/issues"
  },
  "homepage": "https://github.com/chatbotkit/node-sdk#readme",
  "author": "",
  "keywords": [
    "next",
    "auth",
    "nextauth",
    "chatbotkit"
  ],
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    "./contact/adapter": {
      "types": "./types/contact/adapter.d.ts",
      "import": "./src/contact/adapter.js"
    },
    "./contact/adapter.js": {
      "types": "./types/contact/adapter.d.ts",
      "import": "./src/contact/adapter.js"
    },
    "./contact": {
      "types": "./types/contact/index.d.ts",
      "import": "./src/contact/index.js"
    },
    "./contact/index": {
      "types": "./types/contact/index.d.ts",
      "import": "./src/contact/index.js"
    },
    "./contact/index.js": {
      "types": "./types/contact/index.d.ts",
      "import": "./src/contact/index.js"
    },
    "./contact/provider": {
      "types": "./types/contact/provider.d.ts",
      "import": "./src/contact/provider.js"
    },
    "./contact/provider.js": {
      "types": "./types/contact/provider.d.ts",
      "import": "./src/contact/provider.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./index": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./index.js": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./partner/adapter": {
      "types": "./types/partner/adapter.d.ts",
      "import": "./src/partner/adapter.js"
    },
    "./partner/adapter.js": {
      "types": "./types/partner/adapter.d.ts",
      "import": "./src/partner/adapter.js"
    },
    "./partner": {
      "types": "./types/partner/index.d.ts",
      "import": "./src/partner/index.js"
    },
    "./partner/index": {
      "types": "./types/partner/index.d.ts",
      "import": "./src/partner/index.js"
    },
    "./partner/index.js": {
      "types": "./types/partner/index.d.ts",
      "import": "./src/partner/index.js"
    },
    "./partner/provider": {
      "types": "./types/partner/provider.d.ts",
      "import": "./src/partner/provider.js"
    },
    "./partner/provider.js": {
      "types": "./types/partner/provider.d.ts",
      "import": "./src/partner/provider.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:01-cjs": "tsc -p tsconfig.cjs.json",
    "build:01-cjs_rename": "node ../../scripts/rename-cjs.js",
    "build:01-cjs_rewrite_cjs": "node ../../scripts/rewrite-cjs.js",
    "build:01-cjs_rewrite_ts": "node ../../scripts/rewrite-ts.js",
    "build:03-docs": "typedoc",
    "build:02-esm": "tsc -p tsconfig.esm.json",
    "build:00-exports": "node ../../scripts/create-standard-exports.js",
    "build:00-types": "tsc -p tsconfig.types.json",
    "check": "tsc -p tsconfig.check.json",
    "clean": "run-p clean:*",
    "clean:buildinfo": "rimraf *.tsbuildinfo",
    "clean:dists": "rimraf dist",
    "clean:exports": "run-s build:exports",
    "clean:node_modules": "rimraf node_modules",
    "clean:types": "rimraf types",
    "lint": "eslint .",
    "postpack": "node ../../scripts/create-standard-exports.js",
    "prepack": "node ../../scripts/create-dist-exports.js",
    "test": "true"
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@chatbotkit/sdk": "workspace:*",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "typescript": "^5.9"
  },
  "peerDependencies": {
    "npm-run-all": "^4.1.5",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "next-auth": ">=3.0.0",
    "next": ">=13.0.0"
  }
}
