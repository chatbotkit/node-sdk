{
  "name": "@chatbotkit/monorepo",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.16.0"
  },
  "type": "module",
  "workspaces": [
    "packages/fetch",
    "packages/sdk",
    "packages/react",
    "packages/next",
    "system/*",
    "examples/sdk/*",
    "examples/nextjs/*",
    "examples/cloudflare/*"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:all": "turbo run build",
    "build:readme": "cp README.md packages/sdk/README.md",
    "check": "turbo run check",
    "clean": "run-s clean:*",
    "clean:all": "turbo run clean",
    "docs": "run-s docs:*",
    "docs:react": "typedoc --plugin typedoc-plugin-markdown --out ./docs/react --tsconfig ./packages/react/tsconfig.json",
    "docs:sdk": "typedoc --plugin typedoc-plugin-markdown --out ./docs/sdk --tsconfig ./packages/sdk/tsconfig.json",
    "format": "run-s format:*",
    "format:pkg": "format-package -w",
    "format:src": "prettier -w .",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "prepare-changes": "changeset",
    "publish-changes": "turbo run lint check build test && changeset version && changeset publish",
    "test": "turbo run test"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "rimraf": "^5.0.5",
    "eslint": "^8.38.0",
    "typescript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^20.9.1",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint-config-prettier": "^8.8.0",
    "@changesets/cli": "^2.26.2",
    "format-package": "^7.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "turbo": "^1.10.16",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.17.1"
  }
}
